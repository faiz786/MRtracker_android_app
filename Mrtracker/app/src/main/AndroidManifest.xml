<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunbeam.mrtracker">


    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.Signup"></activity>
        <activity android:name=".activity.Login" />
        <activity android:name=".activity.ProductDetails" />
        <activity android:name=".activity.Search" />
        <activity android:name=".activity.Ayurvedic" />
        <activity android:name=".activity.Homoeopathy" />
        <activity android:name=".activity.Allopathic" />
        <activity android:name=".activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>


